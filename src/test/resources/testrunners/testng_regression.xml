<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Regression Test" thread-count="2" parallel="tests"
	verbose="4">

	<listeners>
		<listener class-name="com.qa.opencart.listeners.ExtentListener" />
		<listener class-name="com.qa.opencart.listeners.AllureListener" />
	</listeners>

	<test name="Login Test">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.qa.opencart.test.AccountsPageTest"></class>
		</classes>
	</test>
	
	<test name="Accouts Page Test">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.opencart.test.AccountsPageTest"></class>
		</classes>
	</test>

	


</suite>